<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CreateMenuItemController">
   <children>
      <VBox layoutX="14.0" layoutY="109.0" prefHeight="152.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Label text="Name" />
            <Label text="Calories" />
            <Label text="Protein" />
            <Label text="Price" />
            <Label fx:id="mL" text="Ml." />
         </children>
      </VBox>
      <VBox layoutX="92.0" layoutY="110.0" prefHeight="207.0" prefWidth="149.0" spacing="10.0">
         <children>
            <TextField fx:id="nameEntry" promptText="Name" />
            <TextField fx:id="calorieEntry" promptText="Calories" />
            <TextField fx:id="proteinEntry" promptText="Protein" />
            <TextField fx:id="priceEntry" promptText="Price" />
            <TextField fx:id="miliLiters" promptText="Mililiters" />
         </children>
      </VBox>
      <Button layoutX="37.0" layoutY="352.0" mnemonicParsing="false" onAction="#submitButton" text="Submit" />
      <Label layoutX="191.0" layoutY="13.0" prefHeight="42.0" prefWidth="209.0" text="Create Menu Item">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ListView fx:id="menuItems" layoutX="272.0" layoutY="110.0" prefHeight="195.0" prefWidth="292.0" />
      <Label layoutX="272.0" layoutY="73.0" prefHeight="25.0" prefWidth="189.0" text="Options for Menu" />
      <Label fx:id="errorMessage" layoutX="114.0" layoutY="356.0" />
      <CheckBox fx:id="convertToDrink" layoutX="123.0" layoutY="77.0" mnemonicParsing="false" onAction="#drinkConvert" text="Item Is a drink" />
      <Label layoutX="8.0" layoutY="5.0" />
   </children>
</AnchorPane>
